<form class="ui form center" method="POST" action="/loggedin">
  <h2>Login</h2>
  <div >
      <div class="field">
      <label>Username</label>
      <input required type="text" name="username">
    </div>
    <div class="field">
      <label>Email</label>
      <input required type="text" name="email">
    </div>

    <div class="field">
      <label>Password</label>
      <input required type="password" name="password">
    </div>
  </div>

  <button id= "login" class="button" type="submit">Submit</button>
</form>
</div>